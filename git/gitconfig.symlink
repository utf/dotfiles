[hub]
    protocol = ssh
[user]
  name = Alex Ganose
  email = alexganose@googlemail.com
[alias]
  co = checkout
  lol = log --graph --decorate --pretty=oneline --abbrev-commit --all --color
[color]
    diff = auto
    status = auto
    branch = auto
    ui = true
[core]
    excludesfile = ~/.gitignore
    editor = vim -f
[help]
    autocorrect = 1
